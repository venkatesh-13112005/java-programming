import java.io.*;
import java.util.*;
import java.text.*;
import java.math.*;
import java.util.regex.*;

public class Solution {

    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        int n = sc.nextInt();
        int[]a = new int[n];
        for (int i = 0; i < n; i++) {
            a[i] = sc.nextInt();
        }
        Set<Integer> unique = new HashSet<>();
        for (int nu : a) {
            unique.add(nu);
        }
        int dc = unique.size();
        if (dc==1) {
            System.out.println("There are 1 distinct element in the array.");
        } else {
            System.out.println("There are " + dc + " distinct elements in the array.");
        }
    }
}
