import java.io.*;
import java.util.*;
import java.text.*;
import java.math.*;
import java.util.regex.*;

public class Solution {

    public static void main(String[] args) {
        Scanner obj=new Scanner(System.in);
        int a=obj.nextInt();
        if(a>=1 && a<=9){
            for(int i=1;i<=a;i++){
                System.out.println(a+" x "+i+" = "+(i*a));
            }
        }else {
            System.out.println("Invalid Input");
        }
    }
}
