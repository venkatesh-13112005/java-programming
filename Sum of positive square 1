import java.io.*;
import java.util.*;
import java.text.*;
import java.math.*;
import java.util.regex.*;

public class Solution {

    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);        
        int a=sc.nextInt(); 
        int s=0;
        for (int i=0;i<a;i++)
        {
            int n=sc.nextInt();
            if (n>0) 
            { 
                s+=n*n; 
            }
        }
        System.out.println(s);
    }
}
