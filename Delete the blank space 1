import java.io.*;
import java.util.*;
import java.text.*;
import java.math.*;
import java.util.regex.*;

public class Solution {

    public static void main(String[] args) {
         Scanner obj=new Scanner(System.in);
               String str1=obj.nextLine();
               String str2= str1.replaceAll("\\s", "");
        System.out.println(str2);
    }
}
