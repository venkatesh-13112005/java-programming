import java.util.Scanner;

public class Main {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        int number = sc.nextInt();
        int tens = number / 10;
        int ones = number % 10;
        int sum = tens + ones;
        System.out.println("Bird said:" + number);
        System.out.println("Alice must go in path-" + sum);
       
    }
}
